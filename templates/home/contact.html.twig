{% extends "base.html.twig" %}

{% block title %} {{ title }} {% endblock %}

{% block body %}

	<h1 class="text-center">
		{{ title_page }}
	</h1>
    
	<div class="container">

		{{ form_start(form) }}
		<div class="card">
			<div class="card-body">
				<div class="row">
					<div class="col-md-12">
						{{ form_rest(form) }}
					</div>
				</div>
			</div>
		</div>
		{{ form_end(form) }}

	</div>

{% endblock %}